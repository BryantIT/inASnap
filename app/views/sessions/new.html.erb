<h4>Login</h4>

<%= form_for :user, url: {action: "create"} do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username "Username", :autocomplete => :off %>
    <br>
    <%= f.label :password %>
    <%= f.text_field :password, :placeholder => "******" %>
    <bfr>
    <%= f.submit "Submit" %>
<% end %>

