<%= form_for [photoshoot, location] do |f| %>
    <%= f.label :name "Name" %>
    <%= f.text_field :name %>
    <br>
    <%= f.label :price "Price" %>
    <%= f.text_field :price %>
    <br>
    <%= f.label :client "Client" %>
    <%= f.text_field :client %>
    <br>
    <%= f.submit "Submit" %>
<% end %>